import java.util.Scanner;
import java.io.FileInputStream;

class Main
{
	public static void main(String args[]) 
	{
		Scanner sc = new Scanner(System.in);
		int N =sc.nextInt();
    int result = 0;
    result = fibo(N-1);
    System.out.println(result);

	}
  public static int fibo(int n) {
    if(n<=1){
      return 1; 
    }
    return fibo(n-1)+fibo(n-2);
  }
}